pipeline {
   agent any
     stages {
        stage('Hello') {
        when {
         branch 'master'
         }
            steps {
                echo 'git '
            }
        }
        stage('maven') {
            when {
         branch 'master'
         }
            steps {
                echo 'build World'
            }
        }
        stage('tomcat') {
            when {
         branch 'Aj'
         }
            steps {
                echo 'deploy '
            }
        }
        stage('jenkins') {
            when {
         branch 'bhargav'
         }
            steps {
                echo 'TAT World'
            }
        }
    }
}
