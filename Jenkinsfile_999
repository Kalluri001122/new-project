pipeline {
   agent any
     stages {
        stage('git Hello') {
        when {
         branch 'master'
         }
            steps {
                echo 'git '
            }
        }
        stage('maven ') {
        when {
         branch 'master'
         }
            steps {
                echo 'maven build'
            }
        }
        stage('deploy Hello') {
        when {
         branch 'aj'
         }
            steps {
                echo 'git '
            }
        }
        }
        }
        
